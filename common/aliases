alias gitcp='git cherry-pick'
alias gitr='git reset'
alias gitrh='git reset --hard'
alias gitf='git fetch'
alias gitcpc='git cherry-pick --continue'
alias gitc='git commit -S --signoff'
alias gitp='git push'
alias gitfp='git push -f'
alias gita='git add'

alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'
